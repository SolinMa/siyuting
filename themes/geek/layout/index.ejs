<%- partial('_partial/nav') %>
<div class="hd">
    <% var last; %>
    <% page.posts.each(function (post) { %>
    <% var year = post.date.year(); %>
    <% if (last != year){ %>
    <div class="titlex">
        <p><span class="title-year">#</span>
            <%= year %>
        </p>
    </div>
    <% last = year; %>
    <% } %>
    <a class="flink"
       href="<%- url_for(post.path) %>">
        <div class="list">
            <div class="postname">
                <%= post.title %>
            </div>
            <div class="posttime">
                <%- full_date(post.date, 'MMMM Do') %>
            </div>
        </div>
    </a>
    <% }) %>
    <a class="more"
       href="/archives">More >></a>
</div>
